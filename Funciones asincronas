function setTime() {
    setTimeout(mensaje, 5000);
}





const miPromesa = new Promise ((resolve, reject) => {
    const mensaje = "Hola, soy una promesa";
    if(true) {
        resolve(console.log(mensaje))
    } else {
        reject()
    }
})

