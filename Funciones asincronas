function* generaI() {
    let i = 0;
    while (i<10) {
        i++
        i++
        
        
    if (i===10) {
        return i 
    }
    yield i;
    }
}

const gen = generaI()
console.log(gen.next().value)
console.log(gen.next().value)
console.log(gen.next().value)
console.log(gen.next().value)
console.log(gen.next().value)
